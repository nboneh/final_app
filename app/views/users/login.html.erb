<h1>Login</h1>
<div>
	<div>
    <%= form_for(:session, url: session_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in" %>
    <% end %>

    <p>New user? <%= link_to "Sign up now!", register_path %></p>
  </div>
</div>
